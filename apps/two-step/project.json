{
  "name": "two-step",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/two-step/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/two-step",
        "main": "apps/two-step/src/main.ts",
        "tsConfig": "apps/two-step/tsconfig.app.json",
        "assets": ["apps/two-step/src/assets"],
        "webpackConfig": "apps/two-step/webpack.config.js"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "two-step:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "two-step:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/two-step/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/two-step/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
